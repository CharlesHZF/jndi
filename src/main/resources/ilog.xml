<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <property>
        <name>importsource.jndi.service.scope</name>
        <value>com.importsource.jndi.service</value>
    </property>
   
   
   
   
   
   
    <!--日志输出实现提供者-->
    <property>
        <name>clog.logger</name>
        <value>ConsoleLogger</value>
    </property>
    <!--日志级别 i w e-->
    <property>
        <name>clog.level</name>
        <value>info</value>
    </property>
    <!-- 文件的目录 -->
    <property>
        <name>clog.dir</name>
        <value>/log/</value>
    </property>
    <!-- 文件的前缀 -->
    <property>
        <name>clog.file.pre</name>
        <value>test</value>
    </property>
</configuration>